<%= render '/projects/menu', id:@id%>
<h1> Full Project History </h1>
<br>
<div class='log'>
  <% @logs.each do |log| %>
    <div class='info-detail'>
      <p><b>Time: <%= log.created_at %> </b></p>
      <p><b>Created by: <%= log.getUser %> </b></p>
      <p><%= log.body.gsub(/\n/, '<br/>').html_safe %></p>
    </div>
    <hr>
  <% end %>
</div>
